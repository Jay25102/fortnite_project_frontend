{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password",
        "email"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "password": {
            "type": "string",
            "minLength": 5,
            "maxLength": 25
        },
        "email": {
            "type": "string",
            "format": "email",
            "minLength": 1,
            "maxLength": 60
        }
    }
}